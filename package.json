  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "sculp-js",
    "modules",
    "es6",
    "util"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": "git@github.com:chandq/sculp-js.git",
  "license": "MIT",
  "homepage": "https://github.com/chandq/sculp-js#readme",

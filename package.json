    "rollup-plugin-subpath-externals": "^3.4.0",
    "standard-version": "^9.5.0",
    "typescript": "5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.[tj]s?(x)": [
      "eslint --fix"
    ],
    "*.vue": [
      "eslint --fix"
    ],
    "*.{css,less,scss,json,md}": [
      "prettier --write"
